wp_jssor_3_slider_init = function() {

var wp_jssor_3_options = {$AutoPlay:1,$SlideDuration:800,$SlideEasing:$Jease$.$OutQuint,$ArrowNavigatorOptions:{$Class:$JssorArrowNavigator$},$BulletNavigatorOptions:{$Class:$JssorBulletNavigator$}};

var containerElement = document.getElementById("wp_jssor_3");
containerElement.removeAttribute("id");
wp_jssor_3_slider = new $JssorSlider$(containerElement, wp_jssor_3_options);

/*responsive code begin*/
var MAX_WIDTH = 3000;

function ScaleSlider() {
    var containerElement = wp_jssor_3_slider.$Elmt.parentNode;
    var containerWidth = containerElement.clientWidth;

    if (containerWidth) {

        var expectedWidth = Math.min(MAX_WIDTH || containerWidth, containerWidth);

        wp_jssor_3_slider.$ScaleWidth(expectedWidth);
    }
    else {
        window.setTimeout(ScaleSlider, 30);
    }
}

ScaleSlider();

$Jssor$.$AddEvent(window, "load", ScaleSlider);
$Jssor$.$AddEvent(window, "resize", ScaleSlider);
$Jssor$.$AddEvent(window, "orientationchange", ScaleSlider);
/*responsive code end*/
};
