var coll = document.getElementsByClassName("dropMenu");
var i;

for (i = 0; i < coll.length; i++) {
  coll[i].addEventListener("click", function() {
    this.classList.toggle("actv");
    var cntnt = this.nextElementSibling;
    if (cntnt.style.maxHeight){
      cntnt.style.maxHeight = null;
    } else {
      cntnt.style.maxHeight = cntnt.scrollHeight + "px";
