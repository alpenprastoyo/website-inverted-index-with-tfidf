<script>
            var chart;

            var chartDataPenelitian = [
                				{
                    "year": 2014,
					                    "income": 1166,
					                    "expenses": 512					                },
                				{
                    "year": 2015,
					                    "income": 705,
					                    "expenses": 371					                },
                				{
                    "year": 2016,
					                    "income": 974,
					                    "expenses": 401					                },
                				{
                    "year": 2017,
					                    "income": 1478,
					                    "expenses": 628					                },
                				{
                    "year": 2018,
					                    "income": 1000,
					                    "expenses": 535					                },
                				{
                    "year": 2019,
					                    "income": 1072,
					                    "expenses": 430					                },
                            ];


            AmCharts.ready(function () {
                // SERIAL CHART
                chart = new AmCharts.AmSerialChart();

                chart.dataProvider = chartDataPenelitian;
                chart.categoryField = "year";
                chart.startDuration = 1;
                chart.handDrawnScatter = 3;

                // AXES
                // Category
                var categoryAxis = chart.categoryAxis;
				categoryAxis.labelRotation = 45; // this line makes category values to be rotated
                categoryAxis.gridPosition = "start";
                categoryAxis.gridAlpha = 0.1;
                categoryAxis.axisAlpha = 0;

                // Value
                var valueAxis = new AmCharts.ValueAxis();
                valueAxis.axisAlpha = 0;
                valueAxis.gridAlpha = 0.1;
                valueAxis.position = "left";
                chart.addValueAxis(valueAxis);

                // GRAPHS
                // first graph
                var graph1 = new AmCharts.AmGraph();
                graph1.type = "column";
                graph1.title = "Usulan Penelitian";
                graph1.valueField = "income";
                graph1.balloonText = "Usulan Penelitian:[[value]]";
                graph1.lineAlpha = 0;
                graph1.fillColors = "#ADD981";
                graph1.fillAlphas = 1;
                chart.addGraph(graph1);

                // second graph
                var graph2 = new AmCharts.AmGraph();
                graph2.type = "column";
                graph2.title = "Penelitian Diterima";
                graph2.valueField = "expenses";
                graph2.balloonText = "Penelitian Diterima:[[value]]";
                graph2.lineAlpha = 0;
                graph2.fillColors = "#81acd9";
                graph2.fillAlphas = 1;
                chart.addGraph(graph2);

                // LEGEND
                var legend = new AmCharts.AmLegend();
                chart.addLegend(legend);

                chart.creditsPosition = "top-right";

                // WRITE
                chart.write("graphpenelitian");
            });
</script>



