var rankMath = {"version":"1.0.11","ajaxurl":"https:\/\/arsitektur.ft.uns.ac.id\/wp-admin\/admin-ajax.php","adminurl":"https:\/\/arsitektur.ft.uns.ac.id\/wp-admin\/admin.php","security":"9037402131"};

