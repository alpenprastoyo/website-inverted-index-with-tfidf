if (document.adv.fakultas.value == "14"){document.getElementById('prodi').innerHTML = "<option value=''>Semua Prodi</option><option value='C01'>Sastra Jawa</option><option value='C02'>Sastra Indonesia</option><option value='C03'>Sastra Inggris</option><option value='C05'>Ilmu Sejarah</option><option value='C10'>S-1 Sastra Arab</option><option value='C13'>S-1 Sastra Inggris NR</option><option value='C93'>D3 Sastra Inggris</option><option value='C94'>D3 Usaha Perjalanan wisata</option><option value='C96'>D3 Sastra Tionghwa</option><option value='B01'>S-1 Sastra Jawa</option><option value='B02'>S-1 Sastra Indonesia</option><option value='B03'>S-1 Sastra Inggris</option><option value='B04'>S-1 Ilmu Sejarah</option><option value='B05'>S-1 Sastra Arab</option><option value='B31'>D3 Bahasa Inggris</option>"}
if (document.adv.fakultas.value == "15"){document.getElementById('prodi').innerHTML = "<option value=''>Semua Prodi</option>"}
if (document.adv.fakultas.value == "16"){document.getElementById('prodi').innerHTML = "<option value=''>Semua Prodi</option>"}
 }
</script>

